<% @memes.each do |meme| %>
  <ul class="meme-list">
    <center><li><a href="/memes/<%= meme.id %>"><img src="/<%= meme.photo %>"></a></li></center>
  </ul>
<% end %>


<h2>Submit a meme here!</h2>
<form method="post" action="/memes" enctype="multipart/form-data">
  <input type="file" name="photo">
  <input type="submit">
</form>

